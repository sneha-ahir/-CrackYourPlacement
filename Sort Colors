class Solution {
public:
    void sortColors(vector<int>& nums) {
    int i=0;
    int j=0;
    int n=nums.size();
    for(i=0; i<n-1; i++){
        int min=i;
        for (j=i+1; j<n; j++){
            if(nums[min]>nums[j]){
                min=j;
            }
                 
        }
        swap(nums[min], nums[i]);  
    }
    }
};
